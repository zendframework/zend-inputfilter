{
    "name": "zendframework/zend-inputfilter",
    "description": " ",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "inputfilter"
    ],
    "homepage": "https://github.com/zendframework/zend-inputfilter",
    "autoload": {
        "psr-4": {
            "Zend\\InputFilter\\": "src/"
        }
    },
    "require": {
        "php": ">=5.5",
        "zendframework/zend-filter": "~2.5",
        "zendframework/zend-validator": "dev-feature/new-service-manager",
        "zendframework/zend-stdlib": "dev-develop as 2.8.0"
    },
    "require-dev": {
        "zendframework/zend-servicemanager": "dev-develop as 2.7.0",
        "fabpot/php-cs-fixer": "1.7.*",
        "phpunit/PHPUnit": "^4.5"
    },
    "suggest": {
        "zendframework/zend-servicemanager": "To support plugin manager support"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:ezimuel/zend-validator"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "2.5-dev",
            "dev-develop": "2.6-dev"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZendTest\\InputFilter\\": "test/"
        }
    }
}
